{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "direction": "out",
      "name": "Person",
      "type": "cosmosDB",
      "databaseName": "COSMOS_DB_DATABASE_NAME",
      "collectionName": "COSMOS_DB_COLLECTION_NAME",
      "connectionStringSetting": "COSMOS_DB_CONNECTION_STRING"
    }
  ],
  "scriptFile": "../dist/hello-world/index.js"
}
