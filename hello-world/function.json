{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "todos",
      "type": "cosmosDB",
      "databaseName": "some-db-id",
      "collectionName": "some-container-db",
      "createIfNotExists": true,
      "connectionStringSetting": "OTHER_DB",
      "direction": "out"
    },
    {
      "direction": "out",
      "name": "bier",
      "type": "cosmosDB",
      "databaseName": "ccmsrkazuretf-cosmos-sql-db",
      "collectionName": "beer-container",
      "connectionStringSetting": "COSMOS_DB_CONNECTION_STRING"
    }
  ],
  "scriptFile": "../dist/hello-world/index.js"
}
